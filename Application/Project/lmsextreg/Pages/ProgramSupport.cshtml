@page
@model ProgramSupportModel
@{
    ViewData["Title"] = "Program Support";
}
<h2 class="text-center">@ViewData["Title"]</h2>

<div class="card w-75 mt-5">
    <div class="card-header h4">Fleet Management</div>
    <div class="card-body pb-1">
        <h6 class="card-title">Please contact the Fleet Management team if you have a specific question regarding this program.</h6>
    </div>
    <div class="card-footer">
        <a href="mailto:fleet_training@gsa.gov" target="_blank" class="text-primary font-weight-bold">fleet_training@gsa.gov</a>
